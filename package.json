{
  "name": "react-custom-props",
  "version": "0.3.0",
  "description": "Custom configurable props for React or HTML elements",
  "main": "react-custom-props.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --recursive ./test",
    "coverage": "NODE_ENV=test istanbul cover _mocha -- -R spec",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "clean": "rimraf dist/*",
    "prebuild": "npm run test && npm run clean",
    "build": "NODE_ENV=production webpack -p --colors --progress",
    "watch": "npm run clean && NODE_ENV=dev webpack -d --colors --progress --watch",
    "release": "./scripts/release.sh"
  },
  "files": [
    "/lib",
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sylvaindethier/react-custom-props.git"
  },
  "keywords": [
    "react-custom-props",
    "ReactCustomProps",
    "react",
    "custom",
    "configurable",
    "props",
    "customProps",
    "defaultProps"
  ],
  "author": "Sylvain Dethier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sylvaindethier/react-custom-props/issues"
  },
  "homepage": "https://github.com/sylvaindethier/react-custom-props#readme",
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "react": "^0.14.6"
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "coveralls": "^2.11.6",
    "eslint": "^1.10.3",
    "eslint-config-google": "^0.3.0",
    "eslint-loader": "^1.3.0",
    "expect": "^1.13.4",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.2",
    "rimraf": "^2.5.1",
    "webpack": "^1.12.12"
  }
}
