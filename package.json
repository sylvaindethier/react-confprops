{
  "name": "react-custom-props",
  "version": "0.3.0",
  "description": "Custom configurable props for React or HTML elements",
  "main": "react-custom-props.js",
  "scripts": {
    "test": "npm run test-src && npm run test-lib && npm run test-dist",
    "test-src": "./scripts/test.sh ./test",
    "test-lib": "./scripts/test.sh ./test/lib",
    "test-dist": "NODE_ENV=test mocha ./test/dist",
    "coverage": "NODE_ENV=test istanbul cover _mocha -- -R spec --compilers js:babel-core/register",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "clean": "rimraf dist/*",
    "prebuild": "npm run test-src && npm run clean",
    "build": "npm run build-lib && npm run build-dist",
    "build-lib": "NODE_ENV=production babel ./src -d lib",
    "build-dist": "NODE_ENV=production webpack && NODE_ENV=production MINIFY=true webpack",
    "postbuild": "npm run test-lib && npm run test-dist",
    "postinstall": "npm run build-lib",
    "release": "./scripts/release.sh"
  },
  "files": [
    "src",
    "lib",
    "dist",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sylvaindethier/react-custom-props.git"
  },
  "keywords": [
    "react-custom-props",
    "ReactCustomProps",
    "react",
    "custom",
    "configurable",
    "props",
    "customProps",
    "defaultProps"
  ],
  "author": "Sylvain Dethier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sylvaindethier/react-custom-props/issues"
  },
  "homepage": "https://github.com/sylvaindethier/react-custom-props#readme",
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "react": "^0.14.6"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "coveralls": "^2.11.6",
    "eslint": "^2.1.0",
    "eslint-config-google": "^0.4.0",
    "eslint-config-xo": "^0.10.1",
    "eslint-loader": "^1.3.0",
    "expect": "^1.13.4",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "^2.4.2",
    "rimraf": "^2.5.1",
    "webpack": "^1.12.13"
  }
}
