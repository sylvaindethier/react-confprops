{
  "name": "react-custom-props",
  "version": "1.0.0-alpha.2",
  "description": "Custom configurable props for React or HTML elements",
  "main": "react-custom-props.js",
  "scripts": {
    "clean": "rimraf lib dist coverage",
    "prebuild": "npm run clean && npm run test:src",
    "build:lib": "babel src -d lib",
    "build:dist": "NODE_ENV=development webpack && NODE_ENV=production MINIFY=true webpack",
    "build": "npm run build:lib && npm run build:dist",
    "postbuild": "npm run test:lib && npm run test:dist",
    "test:src": "NODE_ENV=test mocha --compilers js:babel-core/register ./test/src",
    "test:lib": "NODE_ENV=test mocha --compilers js:babel-core/register ./test/lib",
    "test:dist": "NODE_ENV=test mocha ./test/dist",
    "test": "npm run test:src && npm run test:lib && npm run test:dist",
    "coverage": "NODE_ENV=test istanbul cover _mocha -- -R spec --compilers js:babel-core/register",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls",
    "release": "./scripts/release.sh"
  },
  "files": [
    "src",
    "lib",
    "dist",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sylvaindethier/react-custom-props.git"
  },
  "keywords": [
    "react-custom-props",
    "ReactCustomProps",
    "react",
    "custom",
    "configurable",
    "props",
    "customProps",
    "defaultProps"
  ],
  "author": "Sylvain Dethier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sylvaindethier/react-custom-props/issues"
  },
  "homepage": "https://github.com/sylvaindethier/react-custom-props#readme",
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "babel-runtime": "^6.5.0"
  },
  "peerDependencies": {
    "react": "^0.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "coveralls": "^2.11.6",
    "eslint": "^2.1.0",
    "eslint-config-google": "^0.4.0",
    "eslint-config-xo": "^0.10.1",
    "eslint-loader": "^1.3.0",
    "expect": "^1.13.4",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "^2.4.2",
    "rimraf": "^2.5.1",
    "webpack": "^1.12.13"
  }
}
