{
  "name": "react-confprops",
  "version": "0.1.1",
  "description": "Configurable props for React components",
  "main": "react-confprops.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --recursive ./lib/test",
    "test:dist:es6": "NODE_ENV=test mocha --compilers js:babel-core/register --recursive ./dist/test/es6",
    "test:dist:es5": "NODE_ENV=test mocha --recursive ./dist/test/es5",
    "clean": "rimraf dist/*.*",
    "prebuild": "npm run test && npm run clean",
    "build": "NODE_ENV=production webpack -p --colors --progress",
    "postbuild": "npm run test:dist:es6 && npm run test:dist:es5",
    "watch": "npm run clean && NODE_ENV=dev webpack -d --colors --progress --watch",
    "release": "./scripts/release.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sylvaindethier/react-confprops.git"
  },
  "keywords": [
    "react-confprops",
    "ReactConfProps",
    "configurable",
    "react",
    "component",
    "defaultProps"
  ],
  "author": "Sylvain Dethier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sylvaindethier/react-confprops/issues"
  },
  "homepage": "https://github.com/sylvaindethier/react-confprops#readme",
  "dependencies": {
    "react": "^0.14.6"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-config-google": "^0.3.0",
    "eslint-loader": "^1.2.1",
    "expect": "^1.13.4",
    "mocha": "^2.4.2",
    "rimraf": "^2.5.1",
    "webpack": "^1.12.12"
  }
}
